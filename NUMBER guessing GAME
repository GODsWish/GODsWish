#include <iostream>
using namespace std;



int main(){

srand(time(NULL));

int num=(rand()%100)+1;
int guess;
int tries;

do{
cout<<"--------> Number guess game ;)<----------\n";
cout<<"guess the number from 1 to 100 \n";
cin>> guess;
tries++;

if(guess>num){
    cout<<"Amigo you need to guess lower.\n";

}
else if(guess<num){
    cout<<"Amigo you need to guess Higher.\n";
   
}

} while (guess!=num);

cout<<"MY NIGGA you did it in  "<<tries<<" tries";
return 0;
}
